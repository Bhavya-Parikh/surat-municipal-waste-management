<form action="/userGeneration" method="POST">
    <div class="bg_img">
        <div class="page-wrapper bg p-t-130 p-b-100 font-poppins">
            <div class="wrapper wrapper--w680">
                <div class="card card-4">
                    <div class="card-body">
                        <%if(messages.error){%>
                        <span class="text-red-500 text-sm" ><%= messages.error%></span>
                        <%}%>
                        <h2 class="title">Generate User</h2>
                        <form action="/index" method="POST">
                            <div class="row row-space">
                                <div class="col-2">
                                    <div class="input-group">
                                        <label class="label required">Mail Id</label>
                                        <input class="input--style-4" type="text" name="Mail" value="<%=messages.Mail%>" placeholder="Enter Mail Id" required>
                                    </div>
                                </div>
                                <div class="col-2">
                                    <div class="input-group">
                                        <label class="label required">Password</label>
                                        <input class="input--style-4" type="text" value="<%=messages.Password%>" name="Password" placeholder="Enter Password" required>
                                    </div>
                                </div>
                            </div>
                            <div class="row row-space">
                                <div class="col-2">
                                    <div class="input-group">
                                        <label class="label">Role</label>
                                        <input class="input--style-4" type="Role" name="text" value="<%=messages.Role%>" placeholder="Enter User Role" >
                                    </div>
                                </div>
                            <div class="p-t-15">
                                <button class="btn btn--radius-2 btn--blue" type="submit">Submit</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </form>
        <!-- Jquery JS-->
        <script src="vendor/jquery/jquery.min.js"></script>
        <!-- Vendor JS-->
        <script src="vendor/select2/select2.min.js"></script>
        <script src="vendor/datepicker/moment.min.js"></script>
        <script src="vendor/datepicker/daterangepicker.js"></script>
    